{
    "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
    "builds": [
        {
        "src": "api/app.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.12"
        }
        }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/app.py"
      }
    ]
  }
  